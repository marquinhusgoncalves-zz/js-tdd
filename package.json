{
  "name": "js-tdd",
  "version": "1.0.0",
  "description": "Biblioteca Teste TDD",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm run test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": ["text", "html"],
    "exclude": ["tests/**"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marquinhusgoncalves/js-tdd.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Marquinhus Gon√ßalves <eu@marquinhusgoncalves.com> (https://marquinhusgoncalves.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marquinhusgoncalves/js-tdd/issues"
  },
  "homepage": "https://github.com/marquinhusgoncalves/js-tdd#readme",
  "devDependencies": {
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0"
  }
}
